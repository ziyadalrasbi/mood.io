{
  "expo": {
    "name": "moodio",
    "slug": "moodio",
    "scheme": "moodio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/icons/carousel/*",
      "assets/icons/genreselect/*",
      "assets/icons/home/*",
      "assets/icons/login/*",
      "assets/icons/selectmood/*",
      "assets/icons/stats/*",
      "assets/icons/upload/*",
      "assets/icons/*",
      "assets/fonts/Montserrat/*",
      "assets/fonts/Montserrat/static/*"
    ],
    "ios": {
      "bundleIdentifier": "com.app.moodio",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.app.moodio",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
